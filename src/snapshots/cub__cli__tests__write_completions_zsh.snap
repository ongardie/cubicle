_cub_envs() {
    _values -w 'environments' $(cub list --format=names)
}
_cub_pkgs() {
    _values -w 'packages' $(cub package list --format=names)
}
_cub_pkgs_comma() {
    _values -s , -w 'packages' $(cub package list --format=names)
}

if [ "$funcstack[1]" = "_cub" ]; then
